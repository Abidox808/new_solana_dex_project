{
  "version": 2,
  "builds": [
    {
      "src": "solana-dex-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "solana-dex-frontend/dist",
        "buildCommand": "yarn --cwd solana-dex-frontend build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/solana-dex-frontend/$1" 
    }
  ]
}
