{
  "version": 2,
  "builds": [
    {
      "src": "solana-dex-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "cd solana-dex-frontend && yarn install && yarn build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/solana-dex-frontend/dist/$1"
    }
  ]
}
